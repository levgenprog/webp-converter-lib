{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "outDir": "./dist",
    "lib": ["esnext", "dom"],
    "typeRoots": ["node_modules/@types", "./src/types"],
    "baseUrl": "./",
